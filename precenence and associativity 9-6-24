// Precenence and associativity of Operators
#include <stdio.h>

int main() {
    int a = 6, b = 4, c;
    c = a * b + 2;
    printf("Value of c = %d\n", c);
    
    c = a / (b-1) * 3;
    printf("Value of c = %d\n", c);
    
    c = (b+1)%a * 2;
    printf("Value of c = %d\n", c);

    return 0;
}
